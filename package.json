  "scripts": {
    "build": "npm run clean && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch --no-lint",
    "start:debug": "nest start --debug --watch",
